{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-essential" // vue3的规则 https://eslint.vuejs.org/rules/
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "vue/multi-word-component-names": [
      "error",
      {
        // 忽略组件名index检测
        "ignores": ["index"]  
      }
    ]
  }
}
